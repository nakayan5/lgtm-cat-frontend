{
  "name": "lgtm-cat-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 2222",
    "build": "next build",
    "start": "next start -p 2222",
    "lint:prettier": "prettier --cache --check .",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint:eslint": "eslint --cache .",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "lint:next": "next lint",
    "lint:stylelint": "stylelint './**/*.ts{,x}'",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "lint": "run-s lint:*",
    "format": "run-s fix:*",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:ci": "jest --runInBand --collect-coverage",
    "storybook": "start-storybook -s ./public -p 6008",
    "build-storybook": "build-storybook -s public",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@nekochans/lgtm-cat-ui": "^2.0.1",
    "@sentry/nextjs": "^7.12.1",
    "lodash.throttle": "^4.1.1",
    "next": "^12.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "ress": "^5.0.2",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/eslint": "^8.4.6",
    "@types/jest": "^29.0.1",
    "@types/lodash.throttle": "^4.1.7",
    "@types/prettier": "^2.7.1",
    "@types/react": "18.0.19",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "chromatic": "^6.9.0",
    "eslint": "^8.24.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-fetch-mock": "^3.0.3",
    "msw": "^0.47.1",
    "msw-storybook-addon": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "storybook-addon-next": "^1.6.9",
    "stylelint": "14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-smarthr": "^1.1.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-jest": "^29.0.0",
    "typescript": "4.8.3",
