{
  "name": "lgtm-cat-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 2222",
    "build": "next build",
    "start": "next start -p 2222",
    "lint:prettier": "prettier --cache --check .",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint:eslint": "eslint --cache .",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "lint:next": "next lint",
    "lint:stylelint": "stylelint './**/*.ts{,x}'",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "lint": "run-s lint:*",
    "format": "run-s fix:*",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:ci": "jest --runInBand --collect-coverage",
    "generate:css-types": "hcm 'src/**/*.module.{css,scss}'",
    "storybook": "start-storybook -s ./public -p 6008",
    "build-storybook": "build-storybook -s public",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@nekochans/lgtm-cat-ui": "^3.0.0",
    "@sentry/nextjs": "^7.44.2",
    "lodash.throttle": "^4.1.1",
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.6",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/eslint": "^8.21.3",
    "@types/jest": "^29.5.0",
    "@types/lodash.throttle": "^4.1.7",
    "@types/prettier": "^2.7.2",
    "@types/react": "18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "chromatic": "^6.17.2",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-styled-components-varname": "^1.0.1",
    "happy-css-modules": "^2.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "msw": "^1.2.0",
    "msw-storybook-addon": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.6",
    "storybook-addon-next": "^1.7.3",
    "stylelint": "15.3.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-smarthr": "^1.1.0",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-jest": "^29.0.5",
    "typescript": "4.9.5",
    "webpack": "^5.76.3",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
