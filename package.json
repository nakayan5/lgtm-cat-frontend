{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint:prettier": "prettier 'src/**/*.{js,jsx,ts,tsx,css,json}' --check",
    "fix:prettier": "yarn lint:prettier --write",
    "lint:eslint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "fix:eslint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:stylelint": "stylelint 'src/**/*.{css,jsx,tsx}'",
    "fix:stylelint": "stylelint --fix 'src/**/*.{css,jsx,tsx}'",
    "lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:stylelint",
    "format": "yarn fix:prettier && yarn fix:eslint && yarn fix:stylelint",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:ci": "jest --runInBand --collect-coverage",
    "storybook": "start-storybook -s ./public -p 6006",
    "build:storybook": "build-storybook -o build/storybook"
  },
  "dependencies": {
    "bulma": "^0.9.2",
    "next": "^10.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.32.7"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.1.18",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-docs": "^6.1.18",
    "@storybook/addon-knobs": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/addon-storyshots": "^6.1.18",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.18",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@types/prettier": "^2.2.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/stylelint": "^9.10.1",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "css-loader": "^5.0.2",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-stylelint": "^0.4.2",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.5.1",
    "typescript": "4.1.5"
  },
  "license": "MIT"
}
