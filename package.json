{
  "name": "lgtm-cat-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 2222",
    "build": "next build",
    "start": "next start -p 2222",
    "lint:prettier": "prettier --cache --check .",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint:eslint": "eslint --cache .",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "lint:next": "next lint",
    "lint:stylelint": "stylelint --cache './**/*.{css,less,sass,scss}'",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "lint": "run-s lint:*",
    "format": "run-s fix:*",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:ci": "jest --runInBand --collect-coverage",
    "generate:css-types": "hcm 'src/**/*.module.{css,scss}'",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@nekochans/lgtm-cat-ui": "^3.0.1",
    "@sentry/nextjs": "^7.88.0",
    "lodash.throttle": "^4.1.1",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.4",
    "@storybook/nextjs": "^7.6.4",
    "@storybook/react": "^7.6.4",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/eslint": "^8.44.9",
    "@types/jest": "^29.5.11",
    "@types/lodash.throttle": "^4.1.9",
    "@types/prettier": "^2.7.3",
    "@types/react": "18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "chromatic": "^10.1.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "happy-css-modules": "^2.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^1.3.2",
    "msw-storybook-addon": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "storybook": "^7.6.4",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-order": "^6.0.4",
    "ts-jest": "^29.1.1",
    "typescript": "5.3.3",
    "whatwg-fetch": "^3.6.20"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
