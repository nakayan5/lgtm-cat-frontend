{
  "name": "lgtm-cat-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 2222",
    "build": "next build",
    "start": "next start -p 2222",
    "lint:prettier": "prettier --cache --check .",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint:eslint": "eslint --cache .",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "lint:next": "next lint",
    "lint:stylelint": "stylelint --cache './**/*.{css,less,sass,scss}'",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "lint": "run-s lint:*",
    "format": "run-s fix:*",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "generate:css-types": "hcm 'src/**/*.module.{css,scss}'",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@nekochans/lgtm-cat-ui": "^3.0.3",
    "@sentry/nextjs": "^7.100.1",
    "@vercel/edge-config": "^0.4.1",
    "lodash.throttle": "^4.1.1",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@next/env": "^14.1.0",
    "@storybook/addon-essentials": "^7.6.14",
    "@storybook/addon-interactions": "^7.6.14",
    "@storybook/addon-links": "^7.6.14",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.14",
    "@storybook/nextjs": "^7.6.14",
    "@storybook/react": "^7.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/eslint": "^8.56.2",
    "@types/lodash.throttle": "^4.1.9",
    "@types/prettier": "^2.7.3",
    "@types/react": "18.2.55",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.2.2",
    "chromatic": "^10.9.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vitest": "^0.3.22",
    "happy-css-modules": "^2.1.2",
    "jsdom": "^24.0.0",
    "msw": "^2.1.7",
    "msw-storybook-addon": "^2.0.0--canary.122.b3ed3b1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "storybook": "^7.6.14",
    "stylelint": "^16.2.1",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-standard": "^36.0.0",
